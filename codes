local HttpService = game:GetService("HttpService")
local Players = game:GetService("Players")

local player = Players.LocalPlayer
local gui = player:WaitForChild("PlayerGui")

local Config = loadfile("Config.lua")()
local DISCORD_WEBHOOK = Config.WebhookURL
‡∏£‡∏±‡∏ö
local requestFunc = (syn and syn.request) or (http and http.request) or request or http_request
if not requestFunc then
    warn("‚ùå Executor ‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì‡πÑ‡∏°‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö HTTP Request")
    return
end

local function getCash()
    local cashValue = "N/A"
    local cashGui = gui:FindFirstChild("CashGui")
    if cashGui then
        for _, obj in ipairs(cashGui:GetDescendants()) do
            if obj:IsA("TextLabel") and string.find(obj.Text, "%$") then
                cashValue = obj.Text
                break
            end
        end
    end
    return cashValue
end

local function getBankMoney()
    local bankValue = "N/A"
    for _, obj in ipairs(gui:GetDescendants()) do
        if obj:IsA("TextLabel") and string.find(obj.Text, "Bank Balance") then
            bankValue = obj.Text
        end
    end
    return bankValue
end

local function sendToDiscord()
    local data = {
        ["username"] = "BlockSpin | Notification",
        ["avatar_url"] = "https://img2.pic.in.th/pic/file_000000007ca061f7a4998f8f14959e56-removebg-preview.png",
        ["embeds"] = {{
            ["title"] = "üì¢ Map : Block Spin",
            ["description"] = string.format("‚Ä¢ User : %s\n‚Ä¢ Status : %s\n‚Ä¢ Money : %s\n‚Ä¢ Bank Money : %s",
                player.Name,
                "‡∏≠‡∏≠‡∏ô‡πÑ‡∏•‡∏ô‡πå ‚úÖ",
                getCash(),
                getBankMoney()
            ),
            ["color"] = 65280,
            ["thumbnail"] = { ["url"] = "https://img2.pic.in.th/pic/file_000000007ca061f7a4998f8f14959e56-removebg-preview.png" }, -- ‡πÇ‡∏•‡πÇ‡∏Å‡πâ‡∏Ç‡∏ß‡∏≤‡∏ö‡∏ô embed
            ["image"] = { ["url"] = "https://img2.pic.in.th/pic/banner-jopa-studiodc27e67dee058684.gif" },     -- ‡πÅ‡∏ö‡∏ô‡πÄ‡∏ô‡∏≠‡∏£‡πå‡πÅ‡∏ô‡∏ß‡∏¢‡∏≤‡∏ß‡∏•‡πà‡∏≤‡∏á
            ["footer"] = {
                ["text"] = "‚è∞ " .. os.date("%d/%m/%Y %H:%M:%S")
            }
        }}
    }

    local jsonData = HttpService:JSONEncode(data)

    requestFunc({
        Url = DISCORD_WEBHOOK,
        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },
        Body = jsonData
    })
end

while true do
    sendToDiscord()
    wait(600)
end
